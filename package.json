{
  "name": "tree-sitter-ron",
  "version": "0.1.0",
  "description": "Rusty Object Notation (RON) grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "lexer",
    "RON"
  ],
  "author": "Marius Cobzarenco",
  "contributors": [
    "Amaan Qureshi <amaanq12@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amaanq/tree-sitter-ron/issues"
  },
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "eslint-config-google": "^0.14.0",
    "tree-sitter-cli": "^0.20.0"
  },
  "repository": "https://github.com/amaanq/tree-sitter-ron",
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "test": "tree-sitter test && script/parse-examples",
    "parse": "tree-sitter parse",
    "test-windows": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.ron",
      "injection-regex": "ron",
      "file-types": [
        "ron"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}
